    {
      "description": "[modifier] CapsLock enables Layer 1",
      "manipulators": [
        {
          "conditions": [
            {
              "name": "layer_1",
              "type": "variable_if",
              "value": 0
            },
            {
              "name": "layer_2",
              "type": "variable_if",
              "value": 0
            }
          ],
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "layer_1",
                "value": 1
              }
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "layer_1",
                "value": 0
              }
            }
          ],
          "type": "basic"
        }
      ]
    },
